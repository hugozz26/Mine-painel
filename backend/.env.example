# MinePanel Backend Environment Variables
# Copy this file to .env and change all values!

PORT=4000

# JWT secrets — generate strong random strings
JWT_SECRET=change-me-jwt-secret-min-32-chars-long
REFRESH_SECRET=change-me-refresh-secret-min-32-chars-long

# Plugin connection (must match the plugin's config.yml)
PLUGIN_BASE_URL=http://127.0.0.1:8765
PLUGIN_SHARED_SECRET=CHANGE-ME-TO-A-STRONG-SECRET

# SQLite database (Prisma)
DATABASE_URL=file:./minepanel.db

# CORS origin — set to your web panel URL
CORS_ORIGIN=http://localhost:3000
